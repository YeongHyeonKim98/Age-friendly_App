<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity1">
    <ScrollView
        android:id="@+id/ScrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <TextView
        android:id="@+id/textView2"
        android:layout_width="411dp"
        android:layout_height="92dp"
        android:text="자가진단 문진표"
        android:textSize="35sp"
        android:gravity="center"/>
        <Button
            android:id="@+id/btn1_move"
            android:layout_width="match_parent"
            android:layout_height="138dp"
            android:gravity="center"
            android:text="코로나 진단 테스트"
            android:textSize="35sp" />
        <Button
            android:id="@+id/btn2_move"
            android:layout_width="match_parent"
            android:layout_height="138dp"
            android:text="고혈압 진단 테스트"
            android:textSize="35sp"/>
        <Button
            android:id="@+id/btn3_move"
            android:layout_width="match_parent"
            android:layout_height="138dp"
            android:text="치매 진단 테스트"
            android:textSize="35sp"/>
        <Button
            android:id="@+id/btn4_move"
            android:layout_width="match_parent"
            android:layout_height="138dp"
            android:text="당뇨병 진단 테스트"
            android:textSize="35sp"/>

        <Button
            android:id="@+id/btn5_move"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:layout_marginVertical="30dp"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:text="초기화면으로 이동"
            android:textSize="30sp"/>
    </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>